{
  "name": "@racedayready/workouts",
  "version": "0.1.5",
  "description": "Workout schemas, types and utilities.",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/RaceDayReady/workouts"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:fix": "npm run lint:eslint -- --fix && prettier --write .",
    "test": "vitest run",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "release:check": "npm run lint && npm run type-check && npm run build && npm pack --dry-run",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check ."
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.56.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "zod": "^4.3.6"
  }
}
